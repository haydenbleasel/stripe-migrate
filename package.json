{
  "name": "stripe-migrate",
  "packageManager": "pnpm@9.15.4",
  "version": "1.0.16",
  "description": "A Node-based CLI tool to migrate content from between Stripe accounts.",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "stripe-migrate": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haydenbleasel/stripe-migrate.git"
  },
  "keywords": [
    "stripe",
    "migrate",
    "accounts",
    "data"
  ],
  "bugs": {
    "url": "https://github.com/haydenbleasel/stripe-migrate/issues"
  },
  "homepage": "https://github.com/haydenbleasel/stripe-migrate#readme",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "type": "module",
  "author": "Hayden Bleasel <hello@haydenbleasel.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "stripe": "^12.13.0"
  },
  "devDependencies": {
    "@beskar-labs/harmony": "^2.2.8",
    "@biomejs/biome": "2.3.11",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^8.41.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "ultracite": "7.0.8",
    "vitest": "^4.0.16"
  },
  "prettier": "@beskar-labs/harmony/prettier",
